<!--pages/mytask/mytask.wxml-->
<view class="weui-cells weui-cells_after-title">
    <navigator wx:for="{{mytask}}" bindtap="onclick" data-taskid="{{item.taskID}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">
        <view class="weui-media-box__title weui-media-box__title_in-text">{{item.taskName}}</view>
        <view class="weui-media-box__desc">
          <text>截止日期：{{item.deadLine}}</text>
        </view>
      </view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>