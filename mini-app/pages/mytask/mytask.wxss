/* pages/mytask/mytask.wxss */
.button{
  width: 100%;
}
.button button{
  width: 100%;
  height: 100rpx;
  border-width: 2rpx;
  border-color: #EDEDED;
  border-style: solid;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.button button::after{
  border: none;
}
.button image{
  height: 50rpx;
  width: 50rpx;
}